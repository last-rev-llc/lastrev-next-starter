{
  "name": "lastrev-next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --fix \"**/*.js\"",
    "build": "npm run build-content && next build && next export",
    "build:dev": "npm run build-content && netlify build",
    "export": "next export",
    "start": "npm run build-content && netlify dev",
    "static": "next build && serve out",
    "netlify:link": "netlify link",
    "netlify:newfunc": "netlify functions:create",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "build-storybook:static": "build-storybook -c .storybook -o .out && serve .out",
    "build-content": "./build-content"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "@bit/bit.envs.compilers.babel": "file:./components/compilers/babel",
    "@last-rev/adapter-contentful": "^1.0.0",
    "contentful": "^7.14.4",
    "lodash": "^4.17.15"
  },
  "peerDependencies": {
    "bootstrap": "^4.5.0",
    "dotenv": "^8.2.0",
    "next": "^9.4.0",
    "next-redux-wrapper": "^6.0.2",
    "react": "^16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.1.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^26.0.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.2.0",
    "contentful": "^7.14.4",
    "contentful-migration": "^1.1.9",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "install-peers": "^1.0.3",
    "jest": "^26.0.1",
    "netlify-cli": "^2.51.0",
    "next": "^9.4.4",
    "next-redux-wrapper": "^6.0.2",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.18",
      "tester": "bit.envs/testers/jest@22.4.3"
    },
    "overrides": {
      "adapters/*": {
        "env": {
          "compiler": "lastrev.compilers/compilers/babel@7.0.6"
        },
        "dependencies": {
          "@babel/runtime": "^7.10.3"
        }
      },
      "integrations/*": {
        "env": {
          "compiler": "lastrev.compilers/compilers/babel@7.0.6"
        },
        "dependencies": {
          "@babel/runtime": "^7.10.3"
        }
      }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm",
    "resolveModules": {
      "modulesDirectories": [
        "."
      ],
      "aliases": {
        "@components": "src/components",
        "@utils": "src/utils",
        "@adapters": "src/adapters"
      }
    }
  }
}
