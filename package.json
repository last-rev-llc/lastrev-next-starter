{
  "name": "lastrev-next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --fix \"**/*.js\"",
    "build": "npm run build:pre && next build && next export",
    "build:dev": "npm run build:pre && netlify build",
    "export": "next export",
    "start": "npm run build:pre && netlify dev",
    "start:dev": "npm run build:pre && next dev",
    "static": "npm run build:pre && next build && next export && serve out",
    "netlify:link": "netlify link",
    "netlify:newfunc": "netlify functions:create",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "build-storybook:static": "build-storybook -c .storybook -o .out && serve .out",
    "migrate": "node -r dotenv/config migrations/index.js",
    "build:pre": "lr-prefetch && npm run build:paths",
    "build:paths": "node -r dotenv/config build-paths.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "@last-rev/integration-contentful": "^4.1.2-alpha.12",
    "@sentry/react": "^5.20.1",
    "app-root-path": "^3.0.0",
    "bootstrap": "^4.5.0",
    "contentful": "^7.14.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.15",
    "next": "^9.4.0",
    "next-redux-wrapper": "^6.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.1.0",
    "@last-rev/build-prefetch-content": "^4.0.2",
    "@last-rev/generator": "^1.0.8",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/react": "^10.0.4",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.1",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "netlify-cli": "^2.51.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test"
    }
  },
  "lint-staged": {
    "packages/*src/**/*.{js,jsx,json,css,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
